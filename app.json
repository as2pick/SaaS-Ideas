{
    "expo": {
        "name": "SaaS-Ideas",
        "slug": "SaaS-Ideas",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icons/navbar/checklist.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/icons/navbar/checklist.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "ios": {
            "supportsTablet": false
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/icons/navbar/checklist.png",
                "backgroundColor": "#ffffff"
            },
            "package": "fr.as2pick.saasideas"
        },
        "web": {
            "favicon": "./assets/icons/navbar/checklist.png"
        },
        "plugins": ["expo-router", "expo-font"],
        "extra": {
            "router": {
                "origin": false
            }
        }
    }
}
